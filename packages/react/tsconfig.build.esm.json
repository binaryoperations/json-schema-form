{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist/esm",
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "inlineSourceMap": true,
    "paths": {
      "@binaryoperations/json-forms-core": [
        "../core"
      ],
      "#/*": [
        "./*",
        "../core/*"
      ]
    }
  },
  "include": [
    "../core",
    "."
  ]
}