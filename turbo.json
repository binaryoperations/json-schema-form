{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {},
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "../release/dist/core",
        "../release/dist/react"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types",
        "lint"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}